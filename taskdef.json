{
  "executionRoleArn": "arn:aws:iam::837152662068:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/php-sample",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "image": "<IMAGE1_NAME>",
      "name": "php-sample"
    }
  ],
  "secrets": [
    {
      "valueFrom": "DB_HOST",
      "name": "DB_HOST"
    },
    {
      "valueFrom": "DB_NAME",
      "name": "DB_NAME"
    },
    {
      "valueFrom": "DB_PW",
      "name": "DB_PW"
    },
    {
      "valueFrom": "DB_USER",
      "name": "DB_USER"
    }
  ],
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::837152662068:role/ecsTaskExecutionRole",
  "compatibilities": [
    "EC2",
    "FARGATE"
  ],
  "taskDefinitionArn": "arn:aws:ecs:ap-northeast-1:837152662068:task-definition/php-sample:1",
  "family": "php-sample",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX",
    "cpuArchitecture": null
  },
  "cpu": "256",
  "revision": 1,
  "status": "ACTIVE"
}